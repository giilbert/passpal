generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_DB_URL")
}

model User {
  id        String   @id @map("_id") @default(auto()) @db.ObjectId
  createdAt DateTime @default(now())
  name      String
  email     String   @unique
  password  String
}
